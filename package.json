    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^8.0.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
